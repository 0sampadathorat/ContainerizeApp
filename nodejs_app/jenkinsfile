#def clustername = params.ClusterName
def port = params.Port

pipeline {
    agent any
    environment {
        CLUSTER_NAME = "${params.ClusterName}"
        PORT = "${port}"
        
    }

    stages {
        stage('Build') {
            steps {
                sh '''
                    echo "Cluster Name: $CLUSTER_NAME"
                    echo "Port: $PORT"
                    pwd
                '''
            }
        }
    }
}
